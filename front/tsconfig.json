{
    "compilerOptions": {
        "target": "ES5",
        "lib": ["DOM", "ES2015"],
        "module": "ESNext",
        "strict": true,
        "jsx": "react-jsx",
        "moduleResolution": "Node",
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "outDir": "./dist"
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ]
}
